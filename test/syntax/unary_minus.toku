local a = -5;                     # (folded)
assert(a == -5);
a = --5;                    # (folded)
assert(a == 5);
a = ---5;                   # (folded)
assert(a == -5);
a = -a;                     # (not folded)
assert(a == 5);
a = --a;                    # (not folded)
assert(a == 5);
a = ---a;                   # (not folded)
assert(a == -5);
